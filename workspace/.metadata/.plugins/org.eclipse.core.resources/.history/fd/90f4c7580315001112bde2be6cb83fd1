package com.somethingfurry.maps_and_then_some;

import processing.core.*;

import processing.opengl.*;
import codeanticode.glgraphics.*;
import de.fhpotsdam.unfolding.*;
import de.fhpotsdam.unfolding.geo.*;
import de.fhpotsdam.unfolding.utils.*;
import de.fhpotsdam.unfolding.providers.*;


 public class HomelessMapProject extends PApplet {

 Map mappa;

 public void setup(){
	 size(600, 500, GLConstants.GLGRAPHICS);
	 smooth();
	 mappa = new de.fhpotsdam.unfolding.Map(this);
	 MapUtils.createDefaultEventDispatcher(this, mappa);
 }
  
 public void draw(){
	 mappa.draw();
 }


 }


